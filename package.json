{
  "name": "trivia-game",
  "version": "1.0.0",
  "description": "A multiplayer trivia game application",
  "main": "app.js",
  "scripts": {
    "build": "webpack --config ./webpack/prod.config.js",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:client": "webpack-dev-server --config ./webpack/dev.config.js",
    "dev:server": "ts-node-dev --respawn --transpileOnly ./app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:client": "mocha \"src/components/**/test.tsx\" --exit --require ts-node/register ./mocha-setup.js",
    "test:server": "mocha \"src/@(api|sockets)/**/test.ts\" --require ts-node/register"
  },
  "author": "",
  "license": "ISC",
